{
  "name": "Roads",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "dev": "react-native start",
    "and-dev": "react-native run-android && react-native start",
    "dev:cache": "react-native start --reset-cache",
    "prod": "NODE_ENV=production react-native start",
    "prod:cache": "NODE_ENV=production react-native start --reset-cache",

    "build:release": "RN_SRC_EXT=e2e.js detox build -c android.emu.release --reset-cache",
	"build:debug": "detox build -c android.emu.debug",
    "test:release": "detox test -c android.emu.release",
	"test:debug": "detox test -c android.emu.debug",
	"dev:test": "RN_SRC_EXT=e2e.js react-native start --reset-cache",

    "lint": "eslint ."
  },
